{
    "name": "vlotysh/bot-gram-php",
    "description": "PHP client for telegram",
    "type": "library",
    "autoload": {
        "psr-4": {
            "VLotysh\\BotGram\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Vlad Lotysh",
            "email": "lotysh.vm@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.29 || ^9.5.23",
        "mockery/mockery": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "VLotysh\\BotGram\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "run-tests": [
            "composer exec phpunit tests"
        ]
    }
}
